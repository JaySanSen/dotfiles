	"vim.leader": "<space>",
	"vim.useSystemClipboard": true,
	"vim.hlsearch": true,
	"vim.highlightedyank.enable": true,
	"vim.highlightedyank.color": "rgb(130,133,232)",
	"vim.highlightedyank.duration": 200,
	"vim.scroll": 0,
	"vim.autoSwitchInputMethod.enable": false,


	"extensions.experimental.affinity": {
		"vscodevim.vim": 1,
	},
	"vim.handleKeys": {
		"<C-d>": true,
		"<C-s>": true,
		"<C-z>": true,
		"<C-p>": true,
		"<C-f>": true,
		"<C-c>": true,
		"<C-v>": true
	},
	"vim.insertModeKeyBindings": [
		{
			"before": ["j","k"],
			"after": ["<Esc>"]
		}
	],
  "vim.visualModeKeyBindings": [
    {
      "before": ["n","n"],
      "after": ["<Esc>"]
    }
  ],
	"vim.normalModeKeyBindings": [
		{
			"before": ["<leader>","f","f"],
			"commands": ["workbench.action.quickOpen"],
			"silent": true
		},
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["workbench.action.navigateBack"],
      "silent": true
    },
		{
			"before": ["<leader>","n","t"],
			"commands": ["workbench.view.explorer"],
			"silent": true
		},
		{
			"before": ["<leader>","f","d"],
			"commands": ["editor.action.formatDocument"],
			"silent": true
		},
		{
			"before": ["<leader>","c","t"],
			"commands": ["workbench.action.closeActiveEditor"],
			"silent": true
		},
		{
			"before": ["<leader>","s","v"],
			"commands": ["workbench.action.splitEditor"],
			"silent": true
		},
		{
			"before": ["<leader>","e","r"],
			"commands": ["editor.action.rename"],
			"silent": true
		},
		{
			"before": ["<leader>","g","r"],
			"commands": ["editor.action.goToReferences"],
			"silent": true
		},
		{
			"before": ["<leader>","g","i"],
			"commands": ["editor.action.goToImplementation"],
			"silent": true
		},
		{
			"before": ["<leader>","g","h"],
			"commands": ["editor.action.showHover"],
			"silent": true
		},
		{
			"before": ["<leader>","g","s"],
			"commands": ["workbench.action.showAllSymbols"], // for only editor symbols use workbench.action.gotoSymbol
			"silent": true
		},
		{
			"before": ["<leader>","g","d"],
			"commands": ["editor.action.goToDeclaration"],
			"silent": true
		},
		{
			"before": ["<leader>","b","c"],
			"commands": ["editor.action.blockComment"],
			"silent": true
		},
		{
			"before": ["<leader>","l","c"],
			"commands": ["editor.action.commentLine"],
			"silent": true
		},
		{
			"before": ["<leader>","s","c"],
			"commands": ["workbench.action.showCommands"],
			"silent": true
		},
		{
			"before": ["<leader>","f","s"],
			"commands": ["editor.action.formatSelection"],
			"silent": true
		},
		{
			"before": ["<leader>","f","c"],
			"commands": ["editor.fold"],
			"silent": true
		},
		{
			"before": ["<leader>","t","s"],
			"commands": ["workbench.action.toggleSidebarVisibility"],
			"silent": true
		}
	],
	"vim.mouseSelectionGoesIntoVisualMode": false,
	
	
	"files.exclude": {
		"**/.git": false
	},

